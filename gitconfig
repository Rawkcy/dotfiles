[user]
  name = Roxanne Guo
  email = roxanne.guo@me.com

[core]
  editor = /usr/bin/vim

[diff]
  tool = vimdiff

[color]
  ui = auto

[push]
  default = simple

[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green

[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold

[color "status"]
  added = yellow
  changed = green
  untracked = cyan

[alias]
  pullr = pull --rebase
  lol = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  list = show --pretty="format:" --name-only
